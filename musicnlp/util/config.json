    "random-seed": 77,
    "check-arg": [
        {
            "display_name": "Song Representation/Extraction Mode",
            "attr_name": "extract_mode",
            "accepted_values": [
                "melody",
                "full"
            ]
        },
        {
            "display_name": "Extraction Export Type",
            "attr_name": "exp",
            "accepted_values": [
                "mxl",
                "str",
                "id",
                "str_join",
                "visualize"
            ]
        },
        {
            "display_name": "Music File Format",
            "attr_name": "fmt",
            "accepted_values": [
                "mid",
                "mxl"
            ]
        },
        {
            "display_name": "Music File Conversion Backend",
            "attr_name": "backend",
            "accepted_values": [
                "MS",
                "LP"
            ]
        },
        {
            "display_name": "Music Key Type",
            "attr_name": "key_type",
            "accepted_values": [
                "list",
                "enum",
                "dict"
            ]
        },
        {
            "display_name": "Music Channel Mixup Augmentation",
            "attr_name": "channel_mixup",
            "accepted_values": [
                "full",
                "swap"
            ]
        },
        {
            "display_name": "Train Logging Strategy",
            "attr_name": "log_strategy",
            "accepted_values": [
                "epoch",
                "steps",
                "no"
            ]
        },
        {
            "display_name": "Train Logging Mode",
            "attr_name": "log_mode",
            "accepted_values": [
                "train",
                "eval"
            ]
        },
        {
            "display_name": "Generation Mode",
            "attr_name": "generation_mode",
            "accepted_values": [
                "conditional",
                "unconditional"
            ]
        },
        {
            "display_name": "Generation Strategy",
            "attr_name": "generation_strategy",
            "accepted_values": [
                "greedy",
                "sample",
                "contrastive",
                "beam"
            ]
        },
        {
            "display_name": "Distribution Plot Type",
            "attr_name": "dist_plot_type",
            "accepted_values": [
                "bar",
                "hist"
            ]
        },
        {
            "display_name": "Dataset Name",
            "attr_name": "dataset_name",
            "accepted_values": [
                "LMD",
                "LMD-matched",
                "LMD-aligned",
                "LMD-cleaned",
                "LMD-cleaned-subset",
                "MAESTRO",
                "POP909",
                "LMCI",
                "midi-eg",
                "mxl-eg",
                "my",
                "LMD-cleaned-subset"
            ]
        }
